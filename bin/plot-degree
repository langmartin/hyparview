#!/usr/bin/env gnuplot -c
# title = ARG1
# data = ARG2
# SIMULATION_COUNT = ARG3

set title ARG1
set xlabel ARG1
set ylabel "Count Peers"
set ytics "50"
set term png tiny


file(n) = sprintf("data/%05d/%s", n, ARG2)
file_lb(n) = sprintf("%05d/%s", n, ARG2)

plot for [i = 1:ARG3] file(i) with lines title file_lb(i)
